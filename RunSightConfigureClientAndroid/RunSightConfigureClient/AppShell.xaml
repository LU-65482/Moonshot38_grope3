<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="RunSightConfigureClient.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:RunSightConfigureClient"
    xmlns:views="clr-namespace:RunSightConfigureClient.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="RunSightConfigureClient">
    
    <Tab >
        <ShellContent
            Title="首页"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />
        <ShellContent
            Title="连接"
            ContentTemplate="{DataTemplate views:PairNewDevicePage}"
            Route="PairNew" />
    </Tab>
    

</Shell>
